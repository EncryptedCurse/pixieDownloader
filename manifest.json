{
	"manifest_version": 2,
	"name": "pixieDownloader",
	"version": "0.6",
	"icons": {
		"128": "icon/icon128.png"
	},
	"background": {
		"scripts": [ "background.js" ]
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["lib/jsZip.min.js", "content.js"]
		}
	],
	"browser_action": {}
}